<launch>
  
  <!-- Upload URDF file to ros parameter server for rviz to find  -->
  <param name="minimal_rviz_urdf_robot_description" command="$(find xacro)/xacro --inorder '$(find xpp_minimal)/urdf/minimal.urdf'"/>
  
  <!-- Converts Cartesian state to joint state and publish TFs to rviz  --> 
  <node name="urdf_visualizer_minimal" pkg="xpp_minimal" type="urdf_visualizer_minimal" output="screen"/>

</launch>
