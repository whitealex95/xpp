<launch>
 
  <!-- Upload URDF file to ros parameter server for rviz to find  -->
  <param name="aliengo_rviz_urdf_robot_description" command="$(find xacro)/xacro --inorder '$(find xpp_aliengo)/urdf/aliengo.urdf'"/>
  

  <!-- Converts Cartesian state to joint state and publish TFs to rviz  --> 
  <node name="urdf_visualizer_aliengo" pkg="xpp_aliengo" type="urdf_visualizer_aliengo" output="screen"/>
     
</launch>
